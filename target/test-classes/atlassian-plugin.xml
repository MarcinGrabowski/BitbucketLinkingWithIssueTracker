    <version>1.0.0</version>  
    <vendor name="Getin Noble Bank SA" url="https://gnb.pl"/> 
  </plugin-info>  
  <!-- from our base plugin -->  
  <component-import key="myComponent" interface="pl.gnb.plugins.api.MyPluginComponent"/>  
  <!-- from the product container -->  
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>  
  <component key="my-component-wired-test" class="it.pl.gnb.plugins.MyComponentWiredTest" public="true">
    <interface>it.pl.gnb.plugins.MyComponentWiredTest</interface>
